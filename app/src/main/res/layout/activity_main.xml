<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/v_video_crop_seek"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="视频裁剪Seek Bar" />

    <Button
        android:id="@+id/v_shadow_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Shadow view" />

    <Button
        android:id="@+id/v_upload_progress"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="upload progress" />

    <Button
        android:id="@+id/v_scroll_close"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Scroll close" />

    <Button
        android:id="@+id/v_viewmodel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="view model" />

    <Button
        android:id="@+id/v_crop_music"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Crop Music" />

    <Button
        android:id="@+id/v_looper_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Looper list" />

    <Button
        android:id="@+id/v_falling"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Falling" />
    <Button
        android:id="@+id/v_open_gl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Open GL" />
    <com.cj.customwidget.widget.StatusView
        android:id="@+id/v_sta"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:sv_empty_layout_id="@layout/activity_v_m1" />

    <!--    <com.cj.customwidget.widget.SpreadView2-->
    <!--        android:layout_marginTop="100dp"-->
    <!--        android:layout_gravity="center"-->
    <!--        android:layout_width="200dp"-->
    <!--        android:layout_height="200dp"/>-->
</LinearLayout>